local JUGADORSection0 = JUGADOR:NewSection("TikTok:Hackbull.")

local JUGADOR = Window:NewTab("Jugador")
local JUGADORSection1 = JUGADOR:NewSection("Herramientas para el jugador.")

local JUGADOR = Window:NewTab("Herramientas")
local JUGADORSection2 = JUGADOR:NewSection("Herramienta para ganar.")

local JUGADOR = Window:NewTab("Extras")
local JUGADORSection3 = JUGADOR:NewSection("Herramienta extras.")

JUGADORSection1:NewSlider("Walkspeed.", "Speed Change", 250, 16, function(s) -- 250 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

JUGADORSection1:NewSlider("Jump Power.", "Jump Power Change", 500, 50, function(s) -- 250 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

JUGADORSection1:NewButton("Infinite Jump.", "Esta opci√≥n te da salto infinito.", function()
        local thing = Instance.new("Part")
        thing.Name = "This is a part of the game"
        thing.Anchored = true
        thing.Parent = game.Workspace
        thing.Size = Vector3.new(5, 0.1, 5)
        thing.Transparency = 1
    game:GetService("RunService").Stepped:connect(
            function()
            local pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        thing.CFrame = CFrame.new(pos.X, pos.Y-3.3,pos.Z)--beetween -3.1 and idk but prob -3.6
        end)
end)

JUGADORSection1:NewButton("GoodMode.", "Esta opci√≥n de un un Good Mode para evitar tu muerte.", function(state) 
local LocalPlayer = game:GetService("Players").LocalPlayer
 
local function Invincibility()
    if LocalPlayer.Character then
        for i, v in pairs(LocalPlayer.Character:GetChildren()) do
            if v.Name == "hitbox" then
                v:Destroy()
            end
        end
    end
end
 
while wait(0.5) do
    Invincibility(LocalPlayer)
end

end)

JUGADORSection2:NewButton("AutoGanar.", "Esta opci√≥n hace que ganes al momento.", function(state)

local P = game:GetService'Players'
local HRP = P.LocalPlayer.Character.HumanoidRootPart
local Sections = game:GetService("Workspace").tower.sections
 
for _, Child in next, Sections:GetChildren() do
    if Child.Name ~= "start" then
        HRP.CFrame = Child.start.CFrame + Vector3.new(1,2,1)
        task.wait()
    end
end
HRP.CFrame = Sections.finish.FinishGlow.CFrame

end)

JUGADORSection2:NewButton("Obtener todas las herramientas.", "Esta opci√≥n te las herramientas de tower of hell.", function(state)

for _,v in pairs(game.Players.LocalPlayer.Backpack:GetDescendants()) do
        if v:IsA("Tool") then
            v:Destroy()
        end
    end
    wait() 
    for _,v in pairs(game.ReplicatedStorage.Gear:GetDescendants()) do
        if v:IsA("Tool") then
            local CloneThings = v:Clone()
            wait()
            CloneThings.Parent = game.Players.LocalPlayer.Backpack
        end
    end

end)

JUGADORSection3:NewButton("Infinite Yield.", "Esta opci√≥n es un script admin FE.", function(state)
loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
end)

JUGADORSection3:NewButton("Psy.", "Esta opci√≥n es un script ejecutor.", function(state)
loadstring(game:GetObjects("rbxassetid://3014051754")[1].Source)()
end)


